<?xml version="1.0"?>

<DataForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.enova.pl/schema/form.xsd"
          xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/form.xsd"
          RightName="Page:ZmianaNazwyTowarowParamsForm"
          Priority="100"
          DataType="Samples.Example5.Extender.ZmianaNazwTowarowParams">
  <Page Name="ZmianaNazwyTowarowParamsForm" CaptionHtml="Zmiana nazwy towarów" DataContext="{DataSource}">
    <Field CaptionHtml="Typ towaru" EditValue="{TypTowaru}"/>
    <Group CaptionHtml="Dodawania prefix/postfix" LabelWidth="15">
      <Field CaptionHtml="Dodaj prefix" EditValue="{DodajPrefix}" Width="5"/>
      <Field CaptionHtml="Dodaj postfix" EditValue="{DodajPostfix}" Width="5"/>
    </Group>
    <Group CaptionHtml="Usuwanie prefix/postfix" LabelWidth="15">
      <Field CaptionHtml="Usuń prefix" EditValue="{UsunPrefix}" Width="5"/>
      <Field CaptionHtml="Usuń postfix" EditValue="{UsunPostfix}" Width="5"/>
    </Group>
  </Page>
</DataForm>

