      <Field CaptionHtml="SortPodObiekt KodPodObiektu"  EditValue="{SortPodObiekt.KodPodObiektu}" />
      <Field CaptionHtml="SortPodObiekt OpisPodObiektu" EditValue="{SortPodObiekt.OpisPodObiektu}" />
      
      <Field CaptionHtml="SortRelObiektNext KodSortRelObiekt"  EditValue="{SortRelObiektNext.KodSortRelObiekt}" />
      <Field CaptionHtml="SortRelObiektNext NazwaSortRelObiektuVirtual" EditValue="{SortRelObiektNext.NazwaSortRelObiektuVirtual}" />
      
      <Field CaptionHtml="SortRelObiektTop KodSortRelObiekt"  EditValue="{SortRelObiektTop.KodSortRelObiekt}" />
