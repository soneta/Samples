      <Field CaptionHtml="KodObiektu"  EditValue="{KodObiektu}" />
      <Field CaptionHtml="NazwaVirtual" EditValue="{NazwaVirtual}" />
      <Field CaptionHtml="Cena"  EditValue="{Cena}" />
      <Field CaptionHtml="DataObiektu" EditValue="{DataObiektu}" />
      <Field CaptionHtml="Opis" EditValue="{Opis}" />
      <Field CaptionHtml="OkresObiektu" EditValue="{OkresObiektu}" />

      <Field CaptionHtml="KodPocztowy"  EditValue="{SortAdres.KodPocztowy}" />
